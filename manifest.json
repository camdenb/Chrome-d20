{
	"manifest_version" : 2,

	"name": "Tiny D20",
	"short_name": "Tiny D20",
	"description": "A tiny dice set that lives in your toolbar!",
	"version": "1.0",

	"author": "camdenb",

	"omnibox": { "keyword" : "roll"},

	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"browser_action": {
		"default_icon": "icon48.png",
		"default_popup": "popup.html",
		"default_title": "Roll me!"
	},
	"permissions": [
		
	]
}

